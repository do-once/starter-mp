{
    "entryPagePath": "package-main/pages/landing/landing",
    "pages": [
        "package-main/pages/landing/landing"
    ],
    "subPackages": [
        {
            "name":"Static",
            "root": "package-static",
            "pages": [
                "pages/privacy-agreement/privacy-agreement",
                "pages/user-agreement/user-agreement"
            ],
            "independent": true
        },
        {
            "name": "Sub",
            "root": "package-sub",
            "pages": [
                "pages/home/home",
                "pages/login/login"
            ]
        },
        {
            "name": "Sub2",
            "root": "package-sub2",
            "pages": [
                "pages/page-sub2/page-sub2"
            ]
        }
    ],
    "preloadRule": {
        "package-main/pages/landing/landing":{
            "network": "all",
            "packages": ["Sub","Sub2"]
        }
    },
    "window": {
        "navigationBarTextStyle": "black",
        "navigationStyle": "custom"
    },
    "renderer": "webview",
    "rendererOptions": {
        "skyline": {
            "defaultDisplayBlock": true,
            "disableABTest": true,
            "sdkVersionBegin": "3.0.0",
            "sdkVersionEnd": "15.255.255"
        }
    },
    "componentFramework": "exparser",
    "sitemapLocation": "sitemap.json",
    "lazyCodeLoading": "requiredComponents"
}